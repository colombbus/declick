{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "object-literal-sort-keys": false,
    "semicolon": false,
    "space-before-function-paren": [
      true,
      "always"
    ]
  }
}
